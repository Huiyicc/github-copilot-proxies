[req]
distinguished_name = req_distinguished_name
x509_extensions = v3_req
prompt = no
[req_distinguished_name]
C = US
ST = State
L = City
O = Organization
OU = Organizational Unit
CN = *.mycopilot.com
[v3_req]
keyUsage = critical, digitalSignature, keyAgreement
extendedKeyUsage = serverAuth
subjectAltName = @alt_names
[alt_names]
DNS.1 = *.mycopilot.com
DNS.2 = mycopilot.com
DNS.3 = api.mycopilot.com
DNS.4 = copilot-proxy.mycopilot.com
DNS.5 = copilot-telemetry-service.mycopilot.com